<?xml version="1.0"?>
<ProxyGeneratorConfiguration >
  <DescriptionUrl>http://yoursite/api/meta</DescriptionUrl>
  <AdditionalNamespaces>
  </AdditionalNamespaces>
  <GenerateModel>true</GenerateModel>
  <GenerateAttributes>true</GenerateAttributes>
  <ImplementINotifyPropertyChanged>true</ImplementINotifyPropertyChanged>
  <Suffix>Client</Suffix>
  <Namespace>WebApiProxy</Namespace>
  <BaseProxyClass>WebApiProxy.ControllerClient</BaseProxyClass>
  <BaseProxyInterface>WebApiProxy.IControllerClient</BaseProxyInterface>
  <ProxyConstructor>(Uri baseUrl) : base(baseUrl)</ProxyConstructor>
</ProxyGeneratorConfiguration>